import { request } from "./request";
export function login(data: {
	username: string
	password: string
}) {
	return request({
		url: '/api/auth/login',
		method: 'POST',
		data,
	})
}

export function getUserInfo() {
	return request({
		url: '/api/auth/user',
		method: 'GET'
	})
}

export function logout() {
	return request({
		url: '/api/auth/logout',
		method: 'GET'
	})
}